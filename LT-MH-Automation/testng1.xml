<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mobile Tests">
    <listeners>
        <listener class-name="org.magellanhealth.Listeners.TestListener"/>
    </listeners>

    <test name="Test cases" parallel="methods" thread-count="3">
        <classes>
            <class name="org.magellanhealth.testcases.ForgotPasswordTest"/>
            <class name="org.magellanhealth.testcases.LoginTest"/>
            <class name="org.magellanhealth.testcases.RxHomePageTest"/>
            <class name="org.magellanhealth.testcases.MoreTabTest"/>
            <class name="org.magellanhealth.testcases.SignUpTest">
                <methods>
                    <include name="verifySignInScreen"/>
                    <include name="verifyForgotPasswordScreen"/>
                    <include name="verifyElementsOnRxHomePageTest"/>
                    <include name="validateRxHomePage"/>
                    <include name="validateMoreTabTest"/>
                    <include name="SignUpTest"/>
                </methods>
            </class>
        </classes>

    </test>
</suite>




